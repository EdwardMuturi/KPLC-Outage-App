CREATE TABLE place (
name TEXT NOT NULL PRIMARY KEY,
areaId TEXT NOT NULL,
regionId TEXT NOT NULL,
startDate TEXT NOT NULL,
endDate TEXT NOT NULL
);

lectAll:
SELECT *
FROM place;

findByAreaId:
SELECT *
FROM place
WHERE areaId = ?;

findByRegionId:
SELECT *
FROM place
WHERE regionId = ?;

insert:
INSERT INTO place(name, areaId, regionId, startDate, endDate)
VALUES ?;